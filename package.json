{
  "private": true,
  "name": "semver-rust",
  "version": "0.0.0-development",
  "description": "Rust-compatible alternative implementation of semver",
  "keywords": [
    "semver",
    "rust",
    "cargo"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/semver-rust",
  "bugs": "https://github.com/softwareventures/semver-rust",
  "repository": "github:softwareventures/semver-rust",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "tslint --project . && ava"
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "1.11.1"
  },
  "devDependencies": {
    "@softwareventures/tsconfig": "2.2.0",
    "@softwareventures/tslint-rules": "0.7.1",
    "ava": "3.5.0",
    "cz-conventional-changelog": "3.1.0",
    "semantic-release": "17.0.4",
    "ts-node": "8.7.0",
    "tslint": "6.1.0",
    "typescript": "3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
