{
  "private": true,
  "name": "semver-rust",
  "version": "0.0.0-development",
  "description": "Rust-compatible alternative implementation of semver",
  "keywords": [
    "semver",
    "rust",
    "cargo"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/semver-rust",
  "bugs": "https://github.com/softwareventures/semver-rust",
  "repository": "github:softwareventures/semver-rust",
  "license": "ISC",
  "scripts": {
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "tslint --project . && ava"
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@softwareventures/semantic-release-config": "1.1.1",
    "@softwareventures/tsconfig": "2.3.3",
    "@softwareventures/tslint-rules": "0.9.1",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "semantic-release": "17.4.2",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
